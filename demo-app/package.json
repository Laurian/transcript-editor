{
  "name": "transcript-editor-demo-app",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://bbc.github.io/transcript-editor/",
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "immutable-devtools": "0.0.7",
    "react-scripts": "^1.0.10"
  },
  "dependencies": {
    "bootswatch": "^3.3.7",
    "file-saver": "^1.3.2",
    "jss-browserify": "^0.6.0",
    "react-bootstrap": "^0.31.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-overlays": "^0.7.0",
    "react-router": "^2.8.1",
    "transcript-editor": "../",
    "transcript-model": "^0.4.4",
